# gs_gg_sleep211



	
	энергвосст = (1000-$gg['能源'])
	
	if ($clock['小时']*60) > (hour*60):сон = ($clock['小时']*60) - (hour*60) 
	if сон < энергвосст/100*60:
	minutes +=сон
	minutes -= minutes mod 60
	$gg['能源'] +=сон/60*100+сон/60*60
	else
	minutes += энергвосст/100*60
	minutes -= minutes mod 60
	
	$gg['能源'] +=энергвосст+энергвосст/100*60
	end
	

