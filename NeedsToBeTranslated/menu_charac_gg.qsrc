					'|'
					'<a href="exec:my.魅力-=1 & gt ''menu_charac_gg'',''<<my.魅力>>''">-1</a>'
				end
				if my.魅力 - 10 >= 1 :
					'/'
					'<a href="exec:my.魅力-=10 & gt ''menu_charac_gg'',''<<my.魅力>>''">-10</a>'
				end
				if my.魅力 - 100 >= 1 :
					'/'
					'<a href="exec:my.魅力-=100 & gt ''menu_charac_gg'',''<<my.魅力>>''">-100</a>'
				end
			end
		'</p>'
		
		if my.智力 <= 0 :
			my.智力 = 1
			Exp_智力 = 0
		end
		'<p><span class="bold">智力：Lv<<my.智力>>_✭<<$智力[my.智力]>> ▶▶▶ ( <<Exp_智力>>△ / <<needExp_智力>> ) &nbsp;'
			if 开启金手指 > 0:
				'<a href="exec:my.智力+=1 & gt ''menu_charac_gg'',''<<my.魅力>>''">+1</a>'
				'/'
				'<a href="exec:my.智力+=10 & gt ''menu_charac_gg'',''<<my.魅力>>''">+10</a>'


				if my.智力 - 1 >= 1 :
					'|'
					'<a href="exec:my.智力-=1 & gt ''menu_charac_gg'',''<<my.魅力>>''">-1</a>'
				end
				if my.智力 - 10 >= 1 :
					'/'
					'<a href="exec:my.智力-=10 & gt ''menu_charac_gg'',''<<my.魅力>>''">-10</a>'
				end




			end
		'</p>'
		
		'<p><span class="bold">道德：<<my.道德>>&nbsp;'
			if 开启金手指 > 0:
				'<a href="exec:my.道德+=10 & gt ''menu_charac_gg'',''<<my.魅力>>''">+10</a>'
				'/'
				'<a href="exec:my.道德+=100 & gt ''menu_charac_gg'',''<<my.魅力>>''">+100</a>'
				'/'
				'<a href="exec:my.道德+=1000 & gt ''menu_charac_gg'',''<<my.魅力>>''">+1000</a>'
				'|'
				'<a href="exec:my.道德-=10 & gt ''menu_charac_gg'',''<<my.魅力>>''">-10</a>'
				'/'
				'<a href="exec:my.道德-=100 & gt ''menu_charac_gg'',''<<my.魅力>>''">-100</a>'
				'/'
				'<a href="exec:my.道德-=1000 & gt ''menu_charac_gg'',''<<my.魅力>>''">-1000</a>'
			end
		'</p>'
	
		if my.体力 < 0 :
			my.体力 = 0
			Exp_身材 = 0
		end
		'<p><span class="bold">身材：Lv<<my.体力>>_✭<<$身材[my.体力]>> ▶▶▶ ( <<Exp_身材>>△ / <<needExp_身材>> ) &nbsp;'
			if 开启金手指 > 0:
				'<a href="exec:my.体力+=1 & gt ''menu_charac_gg'',''<<my.魅力>>''">+1</a>'
				'/'
				'<a href="exec:my.体力+=10 & gt ''menu_charac_gg'',''<<my.魅力>>''">+10</a>'


				if my.体力 - 1 >= 0 :
					'|'
					'<a href="exec:my.体力-=1 & gt ''menu_charac_gg'',''<<my.魅力>>''">-1</a>'
				end
				if my.体力 - 10 >= 0 :
					'/'
					'<a href="exec:my.体力-=10 & gt ''menu_charac_gg'',''<<my.魅力>>''">-10</a>'
				end




			end
		'</p>'
		
		if my.烹饪 > 16 :
			my.烹饪 = 16
			Exp_烹饪 = needExp_烹饪
		end
		
		if my.烹饪 < 1 :
			my.烹饪 = 1
			Exp_烹饪 = 0
		end
	
		'<p><span class="bold">技能：Lv<<my.烹饪>>_✭<<$烹饪[my.烹饪]>> ▶▶▶ ( <<Exp_烹饪>>△ / <<needExp_烹饪>> ) &nbsp;'
			if 开启金手指 > 0:
				if my.烹饪 + 1 <= 16 :
					'<a href="exec:my.烹饪+=1 & gt ''menu_charac_gg'',''<<my.魅力>>''">+1</a>'
				end
				if my.烹饪 + 10 <= 16 :
					'/'
					'<a href="exec:my.烹饪+=10 & gt ''menu_charac_gg'',''<<my.魅力>>''">+10</a>'
				end
				if my.烹饪 + 1 <= 16 and my.烹饪 - 1 >= 1 :
					'|'
				end
				if my.烹饪 - 1 >= 1 :
					'<a href="exec:my.烹饪-=1 & gt ''menu_charac_gg'',''<<my.魅力>>''">-1</a>'
				end
				if my.烹饪 - 10 >= 1 :
					'/'
					'<a href="exec:my.烹饪-=10 & gt ''menu_charac_gg'',''<<my.魅力>>''">-10</a>'
				end
			end
		'</p>'
	
	
	
		'<p><span class="bold">兼职：<<$健身教练>><<$入境审核员>></p>'
	
		'<p><span class="bold">成就：<<$异世界归来>><<$改写未来之人>><<$钱包鼓鼓者>><<$帝国001>><<$一家之主>><<$校园话事人>><<$电视台之王>><<$荧幕主宰>><<$模范囚犯>><<$魅力异常者>><<$精力异常者>></p>'
		
		if 帝国level < 0 :
			帝国level = 0
			xp = 0
		end
		
		if 帝国level>=0:
			'<p><span class="bold">帝国职位：M<<帝国level>>级 ▶▶▶ 晋升条件：<<xp>>/<<exp>> 经验&nbsp;'
			if 开启金手指 > 0:
				'<a href="exec:帝国level+=1 & gt ''menu_charac_gg'',''<<my.魅力>>''">+1</a>'
				'/'
				'<a href="exec:帝国level+=10 & gt ''menu_charac_gg'',''<<my.魅力>>''">+10</a>'
				'/'
				'<a href="exec:帝国level+=100 & gt ''menu_charac_gg'',''<<my.魅力>>''">+100</a>'
				if 帝国level - 1 >= 0 :
					'|'
					'<a href="exec:帝国level-=1 & gt ''menu_charac_gg'',''<<my.魅力>>''">-1</a>'
				end
				if 帝国level - 10 >= 0 :
					'/'
					'<a href="exec:帝国level-=10 & gt ''menu_charac_gg'',''<<my.魅力>>''">-10</a>'
				end
				if 帝国level - 100 >= 0 :
					'/'
					'<a href="exec:帝国level-=100 & gt ''menu_charac_gg'',''<<my.魅力>>''">-100</a>'
				end
			end
			'</p>'
		end
		
		if $ZZ.money=0 :
			'<p><span class="bold"><font color="#FF3939">政治献金：无</font>&nbsp;'
			if 开启金手指 > 0:
				'<a href="exec:$ZZ.money=1 & gt ''menu_charac_gg'',''<<my.魅力>>''">有</a>'
			end
			'</p>'
		else :
			'<p><span class="bold"><font color="#FF3939">政治献金：非常充足</font>&nbsp;'
			if 开启金手指 > 0:
				'<a href="exec:$ZZ.money=0 & gt ''menu_charac_gg'',''<<my.魅力>>''">无</a>'
			end
			'</p>'
		end
	
	
		'<div class ="charac_quest_txt1">'
			'<p>你好，我叫【<<$主角姓名>>】！'
			if intro_pick=1: 
			'呃(⊙o⊙)…我只知道，我天生就特别聪明，学得飞快。 </p>'
			'我是妈妈生的，但我从没见过她。我的父亲在大山里野炊时遇到了雪豹...已经成植物人2年了。 </p>'
			end
			
			if intro_pick=2: 
			'好了，我知道我很帅，别再盯着我的脸看了！ </p>'
			'听着，我的父亲是一名知名导演，从小就有无数的化妆师，造型师陪我玩耍。耳濡目染的我天生就受人瞩目，我的梦想就是出演男一号！但父亲2年前因为酗酒导致了中风昏迷，这对我来说是个坏消息。</p>'
			end
			
			if intro_pick=3: 
			'如你所见，我老爹很有钱！ </p>'
			'我的老爹？他在一场全球性的疫情中倒卖呼吸机发了一大笔横财，但2年前因为质量问题被患者家属打成了脑梗，现在还躺在ICU里呢....不过，这对我来说其实是个继承遗产的好机会。 </p>'
			end
			
			if intro_pick=4: 
			'从异世界归来是什么感受？老实说那里的妹子可给劲多了！ </p>'
			'我现世的老爹居然还昏迷不醒？拜托，这都过去多久了？看来我得想想办法了....</p>'
			end
			
			'如今父亲不省人事多年，公司也已经停摆许久。是时候有人来 <b>接管他的公司，照顾他的女人了！</b></p>'
			gs '通用修改','金手指修改'
			gs '通用修改','精力和现金修改'
			'<p>
					<a href="exec:$cheat_cmd = input(''直接输入指令<br>警告:可能造成严重问题, 对qgen/qsp不熟的话别乱用'') & dynamic $cheat_cmd">指令</a>
	
			<a href="exec:dynamic $cheat_cmd">重复</a>
			</p>'
			'</div>'
